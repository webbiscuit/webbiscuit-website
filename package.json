{
  "name": "wb_site",
  "version": "1.0.0",
  "description": "Web Biscuit Website",
  "main": "index.html",
  "scripts": {
    "build": "npm run clean && npm run build:webpack-dev && npm run build:metalsmith",
    "build:cv": "npx babel-node build-cv.js",
    "build:prod": "npm run clean && npm run build:webpack-prod && npm run build:metalsmith",
    "build:metalsmith": "babel-node build-metalsmith.js",
    "build:webpack-dev": "npm run clean:webpack && NODE_ENV=development webpack --mode development",
    "build:webpack-prod": "npm run clean:webpack && NODE_ENV=production webpack --mode production",
    "clean": "npm run clean:metalsmith && npm run clean:webpack",
    "clean:metalsmith": "rimraf dist/site/*",
    "clean:webpack": "rimraf dist/assets/*",
    "dev": "NODE_ENV=development babel-node run-dev-server.js",
    "lint": "npm run lint:css && npm run lint:html",
    "lint:html": "npx html-validator-cli --file=dist/site/index.html --verbose -s",
    "lint:css": "npx sass-lint assets/styles/**/*.scss --verbose",
    "prepush": "npm run lint",
    "serve": "browser-sync dist/site",
    "deploy": "aws s3 sync dist/site s3://webbiscuit-site --delete --cache-control max-age=604800"
  },
  "author": "Web Biscuit",
  "repository": {
    "type": "git",
    "url": "git@github.com:webbiscuit/webbiscuit-website.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "bs-fullscreen-message": "^1.1.0",
    "css-loader": "^3.2.0",
    "debug": "^4.1.1",
    "fibers": "^4.0.1",
    "highlightjs": "^9.12.0",
    "html-validator-cli": "^6.0.3",
    "husky": "^3.0.5",
    "jquery": "^3.4.1",
    "jstransformer-nunjucks": "^1.0.0",
    "metalsmith": "^2.3.0",
    "metalsmith-assets": "^0.1.0",
    "metalsmith-fingerprint-ignore": "^2.0.0",
    "metalsmith-layouts": "^2.3.1",
    "metalsmith-markdown": "^1.2.0",
    "metalsmith-metallic": "^2.0.3",
    "metalsmith-permalinks": "^2.2.0",
    "metalsmith-rootpath": "^1.0.4",
    "mini-css-extract-plugin": "^0.8.0",
    "node-pandoc": "^0.3.0",
    "node-sass": "^4.12.0",
    "normalize-scss": "^7.0.1",
    "path": "^0.12.7",
    "popper.js": "^1.15.0",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "sass": "^1.22.12",
    "sass-lint": "^1.13.1",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-middleware": "^3.7.1",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "dependencies": {}
}
